 void l_handle_ALT_rot ( void ) 
 { 
 if ( MCP.r_dir_ALT ) 
 { 
 if ( f_AltTiming ) 
 { 
 wrd_ALT -= 10 ; 
 } 
 else 
 { 
 wrd_ALT -= 1 ; 
 } 
 } 
 else 
 { 
 if ( f_AltTiming ) 
 { 
 wrd_ALT += 10 ; 
 } 
 else 
 { 
 wrd_ALT += 1 ; 
 } 
 } 
 byt_AltTime = 5 ; 
 f_AltTiming = true ; 
 } 
 
 
 void MCP.r_cnt_ALT.On ( void ) { 
 jump ( l_handle_ALT_rot ) ; 
 } 
 
 void MCP.r_cnt_ALT.Off ( void ) { 
 jump ( l_handle_ALT_rot ) ; 
 } 
 
