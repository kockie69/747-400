 ph setHDG ( 15 ) 
 { 
 byte getal1 ; 
 byte getal2 ; 
 word getal3 ; 
 
 wrd_HDG = setHDG.getal3 ; 
 Display.AP_HDG_VALUES = setHDG.getal3 ; 
 } ; 
 
 ph setALT ( 16 ) 
 { 
 byte getal1 ; 
 byte getal2 ; 
 word getal3 ; 
 
 if ( setALT.getal1 == 0xFF ) { Display.AP_ALT_VALUES = blank ; } 
 else { 
 wrd_ALT = setALT.getal3 / 100 ; 
 Display.AP_ALT_VALUES = setALT.getal3 ; 
 } 
 } ; 
 
 ph setSPD ( 17 ) 
 { 
 byte getal1 ; 
 byte getal2 ; 
 word getal3 ; 
 
 if ( setSPD.getal1 == 0xFF ) { Display.AP_SPD_VALUES = blank ; } 
 else { 
 wrd_SPD = setSPD.getal3 ; 
 Display.AP_SPD_VALUES = setSPD.getal3 ; 
 } 
 } ; 
 
 
 ph setVS ( 19 ) 
 { 
 byte getal1 ; 
 byte getal2 ; 
 word getal3 ; 
 
 if ( setVS.getal1 == 0xFF ) { Display.AP_VS_VALUES = blank ; } 
 else { 
 wrd_VS = setVS.getal3 ; 
 if ( ( wrd_VS >> 8 ) > 128 ) { wrd_VS = ( setVS.getal3 - 32768 ) * -1 ; } 
 else { wrd_VS = setVS.getal3 ; } 
 Display.AP_VS_VALUES = wrd_VS ; 
 } 
 } ; 
 
