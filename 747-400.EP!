 void MCP.b_CP_FD.On ( void ) { 
 
 if ( x5414 & 0b0000000000100000 ) 
 { 
 x5414 &= 0b1111111111011111 ; 
 } 
 else 
 { 
 x5414 |= 0b0000000000100000 ; 
 } 
 nqw ( 0x104 , x5414 ) ; 
 } 
 
 void MCP.b_CP_FD.Off ( void ) { 
 
 if ( x5414 & 0b0000000001000000 ) 
 { 
 x5414 &= 0b1111111110111111 ; 
 } 
 else 
 { 
 x5414 |= 0b0000000001000000 ; 
 } 
 nqw ( 0x104 , x5414 ) ; 
 } 
 
